{
  "name": "@pulgueta/nequi-node",
  "description": "SDK de la comunidad de Nequi para Node.js",
  "version": "0.0.0",
  "license": "MIT",
  "author": {
    "name": "Andrés Rodríguez",
    "email": "roariasaf@gmail.com",
    "url": "https://github.com/pulgueta"
  },
  "keywords": [
    "nequi",
    "sdk",
    "nequi-sdk",
    "nequi-node-sdk",
    "bancolombia",
    "pagos",
    "pagos-bancolombia"
  ],
  "homepage": "https://github.com/pulgueta/nequi-node-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/pulgueta/nequi-node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/pulgueta/nequi-node-sdk/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "format": "biome check --write .",
    "lint": "oxlint",
    "ci": "pnpm lint && pnpm format && pnpm build",
    "build": "tsup",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@commitlint/types": "^19.0.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.3.0",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/node": "^20.16.3",
    "conventional-changelog-atom": "^5.0.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "oxlint": "^0.9.2",
    "semantic-release": "^24.1.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=20"
  }
}